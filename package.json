{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "react-boilerplate",
  "private": true,
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "fetch-jsonp": "^1.0.6",
    "history": "^4.6.1",
    "http-proxy-middleware": "^0.17.4",
    "i": "^0.3.5",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^1.1.0",
    "postcss": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "pretty-error": "^2.1.0",
    "prop-types": "^15.5.6",
    "query-string": "^4.3.2",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "source-map-support": "^0.4.14",
    "universal-router": "^3.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-template": "^6.24.1",
    "babel-types": "^6.24.1",
    "browser-sync": "^2.18.8",
    "chokidar": "^1.6.1",
    "css-loader": "^0.28.0",
    "editorconfig-tools": "^0.1.1",
    "file-loader": "^0.11.1",
    "front-matter": "^2.1.2",
    "glob": "^7.1.1",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.3.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-deep-force-update": "^2.0.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "serialize-javascript": "^1.3.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "write-file-webpack-plugin": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "scripts": {
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build --release",
    "build:stats": "yarn run build -- --release --analyse",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "set API_CLIENT_URL=http://localhost:3000&&babel-node tools/run start"
  }
}
